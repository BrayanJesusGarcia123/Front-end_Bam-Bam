{{> sidebar-navbar }}

 <div class="main-content">
        <section class="section">
         
         
          <div class="row">
            <div class="col-12">
              <div class="card">
                <div class="card-header">
                  <h4>Lista de Terceros</h4>
                  
            
               
                </div>
				<div class="card-body">
   <p>A continuacion vera la lista de terceros de la sucursal <b> principal</b></p>
         <div class="table-responsive">
                      <table class="table table-striped table-hover" id="save-stage" style="width:100%;">
                        <thead>
                          <tr>
                            <th>Name</th>
                            <th>Position</th>
                            <th>Office</th>
                            <th>Age</th>
                            <th>Start date</th>
                            <th>Salary</th>
                          </tr>
                        </thead>
                        <tbody>
                          {{#each datos.DATA }}
                          <tr>
                            
                            <td>{{nombre}}</td>
                            <td>{{codigo}}</td>
                            <td> <td ><label class="{{clasifBox tipoterid}}">{{clasif tipoterid}} </label></td></td>
                            <td> <input type="date"  class="form-control" placeholder="{{format fecha}}" value="{{format fecha}}" disabled></td>
                            <td><button type="button" class="btn btn-success btn-sm btn-icon-text " data-toggle="modal"
                                data-target="#exampleModal2">
                                Editar Tercero
                              </button> 
                               <button type="button" class="btn btn-danger btn-sm btn-icon-text" data-toggle="modal"
                                data-target="#exampleModal-{{terceroid}}">
                                Eliminar Tercero
                              </button></td>
                          </tr>
                          {{/each }}
                          
                        </tbody>
                      </table>
                    </div>
               
               </div>
			</div>
		  </div>
          </div>
        </section>

 </div>
     
      

 
